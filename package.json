{
	"name": "mr-flower-pot",
	"description": "A discord and telegram bot.",
  "type": "module",
	"scripts": {
    "install": "bun add discord.js telegraf openai npm-run-all @types/bun --ignore-scripts",
    "start": "bunx npm-run-all -p discord telegram",
    "discord": "bun run discord.ts",
    "telegram": "bun run telegram.ts",
    "debug": "bun run debug.ts"
  },
	"repository": {
		"type": "git",
		"url": "https://github.com/akshaaydev/mr-flower-pot"
	},
	"author": "Akshaay Kaldev",
	"license": "UNLICENSED",
	"dependencies": {
    "@types/bun": "^1.1.3",
    "discord.js": "^14.15.2",
    "npm-run-all": "^4.1.5",
    "openai": "^4.47.2",
    "telegraf": "^4.16.3"
  }
}
